{
  "name": "translation/laravel",
  "description": "Laravel translation made t('simple') with GetText and PHP (key/values) using Translation.io",
  "keywords": [
    "laravel",
    "translation",
    "localization",
    "gettext",
    "translation.io"
  ],
  "homepage": "https://github.com/translation/laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Armands Leinieks",
      "email": "armands.leinieks@gmail.com"
    },
    {
      "name": "MichaÃ«l Hoste",
      "email": "michael.hoste@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "gettext/gettext": "~4.1",
    "illuminate/config": "~5.1",
    "illuminate/console": "~5.1",
    "illuminate/filesystem": "~5.1",
    "illuminate/support": "~5.1",
    "illuminate/translation": "~5.1",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0|~6.0",
    "php-vcr/php-vcr": "^1.3",
    "league/flysystem-vfs": "^1.0",
    "orchestra/testbench": "~3.0",
    "squizlabs/php_codesniffer": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Armandsar\\LaravelTranslationio\\": "src/"
    },
    "files": [
      "src/Helpers/translator_functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Armandsar\\LaravelTranslationio\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit --no-coverage",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Armandsar\\LaravelTranslationio\\ServiceProvider"
      ]
    }
  }
}
